import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int y = 0;
        for(int i=1;i<=n;i++){
            int x = scn.nextInt();
            y = y*10 + x;
        }
        System.out.println(y);
    }
}
