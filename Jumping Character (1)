import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        char ch = scn.next().charAt(0);
        int a = (int)ch;
        if (a>=68 && a<=90){
            System.out.println((char)(a-3));
        }
        else if(a>=97 && a<=119){
            System.out.println((char)(a+3));
        }
        else{
            System.out.println("Can't jump");
        }
    }
}
