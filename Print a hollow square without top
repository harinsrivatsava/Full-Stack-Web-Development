import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        for(int i=1;i<=(n-1);i++){
            System.out.print("*");
            for(int j=1;j<=(n-2);j++){
                System.out.print(" ");
            }
            System.out.print("*");
            System.out.println();
        }
        for(int k=1;k<=n;k++){
            System.out.print("*");
        }
    }
}
