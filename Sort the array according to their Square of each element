import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scn = new Scanner (System.in);
        int N = scn.nextInt();
        Integer [] arr = new Integer [N];
        for (int i = 0; i < N; i++){
            arr [i] = scn.nextInt();
        }
        Arrays.sort( arr , (a,b) -> a*a - b*b);
        for(int j = 0; j < N; j++){
        System.out.print(arr[j] + " ");
    }
    }
    
}
