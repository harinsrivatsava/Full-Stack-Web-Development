import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int N = scn.nextInt();
        int [] reverse = new int[N];
        for(int i=0;i<N;i++){
            reverse[i] = scn.nextInt();
        }
        for(int j=N-1;j>=0;j--){
            System.out.print(reverse[j]+" ");
        }
    }
}
