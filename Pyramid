import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int m =n;
        for(int k=n;k>0;k--){
        for(int i=m-1;i>0;i--){
            System.out.print(" ");
        }
            m--;
        for(int j=n-k+1;j>0;j--){
            System.out.print("* ");
        }
        System.out.println();
    }}
}
