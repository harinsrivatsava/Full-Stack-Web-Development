import java.util.Scanner;
public class Solution{
    public static void main (String [] args){
        Scanner scn = new Scanner (System.in);
        String s1 = scn.next();
        String s2 = scn.next();
        String ans = "";
        for (int i = 0; i < s1.length(); i++){
            ans += s1.charAt(i);
            ans += s2.charAt(i);
        }
        for (int j = 0; j < (2 * s1.length()); j++){
            System.out.print (ans.charAt(j));
        }
    }
}
