import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        String a = scn.next();
        String b = scn.next();
        String c = scn.next();
        String d = a+b+c;
        char ch = d.charAt(2);
        int len = d.length();
        System.out.println(len);
        System.out.println(ch);
    }
}
