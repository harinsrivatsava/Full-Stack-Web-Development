import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        System.out.print(n+" ");
        int m = n/3;
        for (int i=1;m>0;i++){
            System.out.print(m+" ");
            m = m/3;
        }
    }
}
