import java.util.Scanner;
public class Solution{
    public static void main (String [] args){
        Scanner scn = new Scanner (System.in);
        String s = scn.nextLine();
        int n = s.length();
        for (int k = 0; k <= (n - 1); k++){
        for (int i = k; i <= (n - 1); i++){
            for (int j = k; j <= i; j++){
                System.out.print (s.charAt (j));
            }
            System.out.println();
        }}
    }
}
