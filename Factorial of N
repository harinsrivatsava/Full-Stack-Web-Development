import java.util.Scanner;
public class Solution { 
    public static int factorial(int n){
        int fact=1;
        for(int i=1;i<=n;i++){
            fact*=i;
        }
        return fact;
    }
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n =scn.nextInt();
        int fn = factorial(n);
        System.out.println(fn);
        }
}
