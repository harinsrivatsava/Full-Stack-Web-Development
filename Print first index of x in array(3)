import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int [] array = new int [n];
        for (int i=0;i<n;i++){
            array[i] = scn.nextInt();
        }
        int m = scn.nextInt();
        System.out.println(first_index(array, m));       
        }
    public static int first_index(int [] array, int m){
        for(int j=0;j<array.length;j++){
            if(array[j] == m){
                return j;
            }
        }
        return -1;
        } 
    }
