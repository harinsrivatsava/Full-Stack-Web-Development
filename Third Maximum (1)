import java.util.Scanner;
import java.util.Arrays;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner (System.in);
        int N = scn.nextInt();
        int [] nums = new int [N];
        for (int i=0; i<N; i++){
            nums[i] = scn.nextInt();
        }
        Arrays.sort(nums);
        if (N < 3){
            System.out.println(nums[N-1]);
        }
        else{
            System.out.println(nums[N-3]);
        }
    }
}
