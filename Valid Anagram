import java.util.Scanner;
public class Solution{
    public static void main (String [] args){
        Scanner scn = new Scanner (System.in);
        String S = scn.next();
        String T = scn.next();
        int s = S.length();
        int t = T.length();
        int [] freqS = new int [26];
        int [] freqT = new int [26];
        for (int i = 0; i < s; i++){
            char chS = S.charAt(i);
            freqS [chS - 'a']++;
        }
        for (int i = 0; i < t; i++){
            char chT = T.charAt(i);
            freqT [chT - 'a']++;
        }
        for (int j = 0; j < 26; j++){
            if (freqS [j] != freqT [j]){
                System.out.println ("No");
                return;
            }
        }
        System.out.println ("Yes");
    }
}
