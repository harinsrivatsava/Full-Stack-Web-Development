import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int x = scn.nextInt();
        int y = scn.nextInt();
        int w = scn.nextInt();
        int z = scn.nextInt();
        int a = x*y;
        int b = w*z;
        boolean ans = (a==b);
        System.out.println(ans);
    }
}
