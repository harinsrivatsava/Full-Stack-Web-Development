import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int a = scn.nextInt();
        int s = scn.nextInt();
        if (a > 60){
            System.out.println(s+1000);
        }
        else{
            if (a>40 && a <=60){
            System.out.println(s+500);
        }
        else{
            System.out.println(s);
        }
        }
    }
}
