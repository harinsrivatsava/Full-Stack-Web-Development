import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int a = scn.nextInt();
        int b = scn.nextInt();
        int c = scn.nextInt();
        if (a>b && a>c){
            System.out.println("A");
        }
        else{
            if (b>a && b>c){
                System.out.println("B");
            }
            else{
                System.out.println("C");
            }
        }
    }
}
