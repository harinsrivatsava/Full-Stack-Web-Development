import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scn = new Scanner (System.in);
        int n = scn.nextInt();
        int tar = scn.nextInt();
        HashMap<Integer, Integer> hm = new HashMap <> ();
        for (int i = 0; i < n; i++){
            int key = scn.nextInt();
            int rem = tar - key;
            if (hm.containsKey(rem)){
                System.out.print(hm.get(rem) + " " + i);
                break;
            }
            hm.put(key, i);
        }
    }
}
