import java.util.Scanner;
public class Solution{
    public static void main (String [] args){
    Scanner scn = new Scanner (System.in);
    String S = scn.nextLine();
    int n = S.length();
    int count = 0;
    for (int i = (n - 1); i >= 0; i--){
        if (S.charAt(i) != ' '){
            count++;
        }
        else{
            break;
        }
    }
    System.out.println (count);
    }
}
