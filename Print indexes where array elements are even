import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int [] even_array = new int [n];
        for(int i=0;i<n;i++){
            even_array[i] = scn.nextInt();
            if(even_array[i]%2==0){
                System.out.print(i+" ");
            }
        }
    }
}
