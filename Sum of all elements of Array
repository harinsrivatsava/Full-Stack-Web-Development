import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int [] array = new int [n];
        for(int i=0;i<n;i++){
            array[i] = scn.nextInt();
        }
        System.out.println(sum(array));
    }
    public static int sum(int [] A){
        int count = 0;
        for(int i=0;i<A.length;i++){
            count = count+A[i];
        }
        return count;
    }
}
