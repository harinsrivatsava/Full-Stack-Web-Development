import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int T = scn.nextInt();
        int x = 0;
        for(int i=1;i<=T;i++){
            int n = scn.nextInt();
            while(n>0){
                x = x*10+(n%10);
                n = n/10;
            }
            System.out.println(x);
            x = 0;
        }
    }
}
