import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int [] odd_array = new int [n];
        for(int i=0;i<n;i++){
            odd_array[i] = scn.nextInt();
        }
        for(int j=0;j<n;j++){
            if(odd_array[j]%2 == 1){
                System.out.print(j+" ");
            }
        }
    }
}
