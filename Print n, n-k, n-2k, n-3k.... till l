import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int N = scn.nextInt();
        int K = scn.nextInt();
        int L = scn.nextInt();
        for (int i=0;(N-(i*K))>=L;i++){
            System.out.println(N-(i*K));
        }
    }
}
