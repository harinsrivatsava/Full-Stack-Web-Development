import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int m = 1;
        System.out.print(1+" ");
        for(int i=1;m<=n;i++){
                m*=2;
            if(m<=n){    
            System.out.print(m+" ");
        }
        }
    }
}
