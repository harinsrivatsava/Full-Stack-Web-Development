import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int T = scn.nextInt();
        for(int i=1;i<=T;i++){
            int n = scn.nextInt();
            int res = 0;
            while(n>0){
                int digit = n%10;
                res = res*10+digit;
                n = n/10;
            }
            System.out.println(res);
        }
    }
}
