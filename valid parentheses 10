import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scn = new Scanner (System.in);
        String s = scn.next();
        Stack <Character> st = new Stack <> ();
        for (int i = 0; i < s.length(); i++){
            char ch = s.charAt(i);
            if (ch == '(' || ch == '{' || ch == '['){
                st.push(ch);
            }
            else{
                if (st.size() == 0){
                    System.out.println (false);
                    return;
                }
                else if (ch == ')' && st.peek() != '('){
                    System.out.println (false);
                    return;
                }
                else{
                    st.pop();
                }
            }
        }
        System.out.println (st.size() == 0);
    }
}
