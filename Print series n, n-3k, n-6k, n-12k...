import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int k = scn.nextInt();
        System.out.print(n+" ");
        for(int i=3;(n-i*k)>0;i+=3){
            System.out.print(n-k*i+" ");
        }
    }
}
