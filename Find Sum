import java.util.Scanner;
public class Solution{
    public static int sum(int x,int y){
        int s = x+y;
        return s;
    }
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int T = scn.nextInt();
        for(int i=1;i<=T;i++){
            int x = scn.nextInt();
            int y = scn.nextInt();
            System.out.println(sum(x,y));
        }
    }
}
