import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int N = scn.nextInt();
        int [] Arr = new int [N];
        for(int i=0;i<N;i++){
            Arr[i] = scn.nextInt();
        }
        prod(Arr);
    }
    public static void prod(int [] A){
        int p = 1;
        for(int i=0;i<A.length;i++){
            p = p*A[i];
        }
        System.out.println(p);
    }
}
