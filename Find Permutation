import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int r = scn.nextInt();
        int m = 1;
        int q = 1;
        for(int i=1;i<=n;i++){
            m*=i;
        }
        for(int j=1;j<=(n-r);j++){
            q*=j;
        }
        System.out.println((int)(m/q));
    }
}
