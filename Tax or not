import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int i = scn.nextInt();
        if (i<500000){
            System.out.println("No tax to be paid");
        }
        else{
            System.out.println("Tax to be paid");
        }
    }
}
