import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
            int n = scn.nextInt();
            int [] div3 = new int[n];
            for(int i=0;i<n;i++){
                div3[i] = scn.nextInt();
            }
            for(int j=0;j<n;j+=3){
                System.out.print(div3[j]+" ");
            }
        }
    }
