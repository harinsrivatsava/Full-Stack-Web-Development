import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int y = n;
        int count = 0;
        while(y>0){
            y = y/10;
            count++;
        }
        System.out.println(count);
        while(n>0){
            System.out.println(n%10);
            n = n/10;
        }
    }
}
