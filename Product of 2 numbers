import java.util.Scanner;
public class Solution{
    public static int prod(int x, int y){
        int multiply = x*y;
        return multiply;
    }
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int T = scn.nextInt();
        for(int i=1;i<=T;i++){
            int x = scn.nextInt();
            int y = scn.nextInt();
            int m = prod(x,y);
            System.out.println(m);
        }
    }
}
