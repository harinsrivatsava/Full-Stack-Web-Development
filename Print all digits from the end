import java.util.Scanner;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int a = 0;
        int c = 0;
        int b = n;
        for (int i=1;b>0;i++){
            b=b/10;
            a++;
        }
        for (int j=1;j<=a;j++){
            c = n%10;
            n = n/10;
            System.out.println(c);
        }}}
