import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        String str = scn.next();
        int l = str.length();
        for(int i=0;i<l;i++){
            char c = str.charAt(i);
            if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u'){
                System.out.println(c);
            }
        }
    }
}
