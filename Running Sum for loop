import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        long m = 0;        
        for(int i=1;i<=n;i++){
            int num = scn.nextInt();
            m+=num;
            System.out.print(m+" ");            
        }
    }
}
