import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int [] alternate = new int[n];
        for(int i=0;i<n;i++){
            alternate[i] = scn.nextInt();
        }
        for(int j=0;j<n;j+=2){
            System.out.println(alternate[j]);
        }
    }
}
