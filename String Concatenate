import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        String s1 = scn.next();
        String s2 = scn.next();
        int l1 = s1.length();
        int l2 = s2.length();
        if(l1<l2){
            System.out.println(s1+s2+s1);
        }
        else{
            System.out.println(s2+s1+s2);
        }
    }
}
