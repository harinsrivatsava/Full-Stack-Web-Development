import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int j = 2;
        int m = 1;
        for(int i=1;i<=n;i++){
            m*=j;
        }
        System.out.println(m);
    }
}
