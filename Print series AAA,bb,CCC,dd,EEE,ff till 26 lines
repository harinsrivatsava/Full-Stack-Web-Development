import java.util.*;
public class Solution{
    public static void main(String[] args){
        char k ='A';
        char j ='b';
        for(int i=1;i<=26;i++){
        if(i%2==1){
            System.out.print(k);
            System.out.print(k);
            System.out.println(k);
            k+=2;
        }
        else{
            System.out.print(j);
            System.out.println(j);
            j+=2;
        }
    }}
}
