import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        char ch = scn.next().charAt(0);
        if ((int)ch>=65 && (int)ch<=90){
            System.out.println((char)((int)ch+32));
        }
        else if ((int)ch>=97 && (int)ch<=122){
            System.out.println((char)((int)ch-32));
        }
    }
}
